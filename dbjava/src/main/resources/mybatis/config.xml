<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD config 3.0//EN" "https://mybatis.org.dtd/mybatis-3-config.dtd">

<configuration>
	<settings>
		<setting name="cacheEnabled" value="false"/>
		<setting name="useGeneratedKeys" value="false"/>
		<setting name="mapUnderscoreToCamelCase" value="false"/>
	</settings>
	<typeAliases>
		<typeAlias type="com.javabang.model.MemberDTO" alias="member" ></typeAlias>
		<typeAlias type="com.javabang.model.RentDTO" alias="rent"></typeAlias>
		<typeAlias type="com.javabang.model.ReviewDTO" alias="review"></typeAlias>
		<typeAlias type="com.javabang.model.ReservationDTO" alias="reservation"></typeAlias>
		<typeAlias type="com.javabang.model.PagingDTO" alias="paging"></typeAlias>
		<typeAlias type="com.javabang.model.ReportDTO" alias="report"></typeAlias>
		<typeAlias type="com.javabang.model.ReviewReportDTO" alias="reviewReport"></typeAlias>
	</typeAliases> 
</configuration>

